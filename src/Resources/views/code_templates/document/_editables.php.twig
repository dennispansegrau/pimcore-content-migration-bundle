{% autoescape false %}
{% for editable in editables %}
{% if editable is instanceof('PimcoreContentMigration\\Writer\\RelativePath') %}
            ->loadWysiwygFromPath('{{ editable.getName() }}', __DIR__ . '{{ editable.getRelativePath() }}')
{% elseif editable.type == "image" %}
            ->setEditable({{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createImage('{{ editable.getName() }}', {{ pcmb_value_to_string(editable.getImage(), dependencies) }}, '{{ editable.getText() }}', {{ editable.getCropPercent() ? 'true' : 'false' }}, {{ editable.getCropWidth() }}, {{ editable.getCropHeight() }}, {{ editable.getCropTop() }}, {{ editable.getCropLeft() }}, {% if editable.getHotspots() is empty %}[]{% else %}[
{% for hotspot in editable.getHotspots() %}
                {{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createHotspot({{ hotspot.top }}, {{ hotspot.left }}, {{ hotspot.width }}, {{ hotspot.height }}, {% if hotspot.data is empty %}[]{% else %}[
{% for markerHotspotItems in hotspot.data %}
                    {{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createMarkerHotspotItem('{{ markerHotspotItems.name }}', '{{ markerHotspotItems.type }}', {{ pcmb_value_to_string(markerHotspotItems, dependencies) }}),
{% endfor %}
                ]{% endif %}, {{ pcmb_value_to_string(hotspot.name, dependencies) }}),
{% endfor %}
            ]{% endif %}, {% if editable.getMarker() is empty %}[]{% else %}[
{% for marker in editable.getMarker() %}
                {{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createMarker({{ marker.top }}, {{ marker.left }}, {% if marker.data is empty %}[]{% else %}[
{% for markerHotspotItems in marker.data %}
                    {{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createMarkerHotspotItem('{{ markerHotspotItems.name }}', '{{ markerHotspotItems.type }}', {{ pcmb_value_to_string(markerHotspotItems, dependencies) }}),
{% endfor %}
                ]{% endif %}, {{ pcmb_value_to_string(marker.name, dependencies) }}),
{% endfor %}
            ]{% endif %}))
{% elseif editable.type == "input" %}
            ->setEditable({{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createInput('{{ editable.name }}', {{ pcmb_value_to_string(editable.data, dependencies) }}))
{% elseif editable.type == "textarea" %}
            ->setEditable({{ '\\PimcoreContentMigration\\Builder\\Document\\EditableBuilder' }}::createTextarea('{{ editable.name }}', {{ pcmb_value_to_string(editable.data, dependencies) }}))
{% else %}
            ->setRawEditable('{{ editable.name }}', '{{ editable.type }}', {{ pcmb_value_to_string(editable.data, dependencies) }})
{% endif %}
{% endfor %}
{% endautoescape %}